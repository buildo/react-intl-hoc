    "component"
  ],
  "author": "Giovanni Gonzaga <giovanni@buildo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildo/react-intl-hoc/issues"
  },
  "homepage": "https://github.com/buildo/react-intl-hoc",
  "dependencies": {
    "react-intl": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.11",
    "babel-loader": "^5.1.3",
    "eslint": "^0.22.1",
    "eslint-loader": "^0.12.0",
    "eslint-plugin-react": "^2.5.0",
    "expect": "^1.6.0",
    "isparta-loader": "^0.2.0",
    "karma": "^0.12.36",
    "karma-bamboo-reporter": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "require-dir": "^0.3.0",
    "webpack": "^1.9.10"
